package start;

import jade.Boot3;

public class Start {

	private static final int CLIENTS_AMOUNT = 3;
	private static final int MORRIS_AMOUNT = 1; //system aukcyjny
	
	public static void main(String args[]) {
		
		int all = 1 + MORRIS_AMOUNT + CLIENTS_AMOUNT;
		String[] param = new String[all];

		param[0] = "-gui";
		
		for (int i = 1; i < MORRIS_AMOUNT + 1; ++i) {
			param[i] = "MorrisColumn"+ i +":agents.MorrisColumn";
		}
		for (int i = MORRIS_AMOUNT + 1; i < MORRIS_AMOUNT + CLIENTS_AMOUNT + 1; ++i) {
			param[i] = "ClientAgent"+ i +":agents.ClientAgent";
		}

		new Boot3(param);
		
	}

	
}

package data;

import java.io.Serializable;

public class Preference implements Serializable {

	private String type;
	private double maxcena;
	private int startSec; // licytacja od czasu (od konca ilosc sekund) lub 0 to od razu
	private boolean cenapriority;
	private boolean datapriority;
	private int timeBid; /*jak czesto podbijac cene*/
	private Strategy strategy;
	private Priority prioty;
	
	
	
	public Preference() {
		setTimeBid(0);
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public double getMaxcena() {
		return maxcena;
	}

	public void setMaxcena(double maxcena) {
		this.maxcena = maxcena;
	}

	public boolean isCenapriority() {
		return cenapriority;
	}

	public void setCenapriority(boolean cenapriority) {
		this.cenapriority = cenapriority;
	}

	public boolean isDatapriority() {
		return datapriority;
	}

	public void setDatapriority(boolean datapriority) {
		this.datapriority = datapriority;
	}

	public int getStartSec() {
		return startSec;
	}

	public void setStartSec(int startSec) {
		this.startSec = startSec;
	}

	public int getTimeBid() {
		return timeBid;
	}

	public void setTimeBid(int timeBid) {
		this.timeBid = timeBid;
	}

	public Priority getPrioty() {
		return prioty;
	}

	public void setPrioty(Priority prioty) {
		this.prioty = prioty;
	}

	public Strategy getStrategy() {
		return strategy;
	}

	public void setStrategy(Strategy strategy) {
		this.strategy = strategy;
	}

}

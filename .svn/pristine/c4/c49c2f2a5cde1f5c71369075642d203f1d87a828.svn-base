package data;

import java.io.Serializable;

public class Announce implements Serializable {
	private int id;
	private String typ;
	private double cena;
	
	public Announce(String typ, double cena, int id){
		this.cena = cena;
		this.id = id;
		this.typ = typ;
	}
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getTyp() {
		return typ;
	}
	public void setTyp(String typ) {
		this.typ = typ;
	}
	public double getCena() {
		return cena;
	}
	public void setCena(double cena) {
		this.cena = cena;
	}

}
